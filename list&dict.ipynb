   "source": [
    "def fibonacci_dict(n):\n",
    "    fib = [0, 1]\n",
    "    while fib[-1] + fib[-2] < n:\n",
    "        fib.append(fib[-1] + fib[-2])\n",
    "    \n",
    "    result = {}\n",
    "    for x in range(n + 1):\n",
    "        result[x] = [f for f in fib if f < x]\n",
    "    \n",
    "    return result\n",
    "\n",
    "print(fibonacci_dict(5))\n"
   ]
  }
